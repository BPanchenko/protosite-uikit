{
  "name": "protosite.uikit",
  "version": "1.7.41",
  "description": "Front-end framework for developing web interfaces using BEMIT",
  "author": "Boris Panchenko <hello@bpanchenko.ru>",
  "main": "./index.html",
  "scripts": {
    "dev": "webpack-dev-server --mode development",
    "build": "webpack --mode production"
  },
  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite.uikit/issues"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "cssnano": "^3.10.0",
    "eslint": "^5.2.0",
    "less": "^3.8.0",
    "postcss": "^6.0.20",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0",
    "webpack-dev-server": "^3.1.5"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "esprima",
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [
        2,
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "homepage": "https://github.com/BPanchenko/protosite.uikit#readme",
  "license": "ISC",
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite.uikit.git"
  }
}
