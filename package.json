{
	"name": "@bpanchenko/uikit",
	"version": "4.8.26",
	"description": "CSS framework for developing web interfaces",
	"homepage": "https://github.com/BPanchenko/protosite-uikit#readme",
	"type": "module",
	"main": "main.css",
	"files": [
		".config/postcss.config.cjs",
		"component/*",
		"document/*",
		"object/*",
		"setting/*",
		"style/*",
		"theme/*",
		"utility/*"
	],
	"scripts": {
		"build": "run-p main document components styles utilities objects theme-document",
		"components": "postcss ./component/main.css --output ./assets/components.css --config ./.config/postcss.config.cjs --verbose",
		"document": "postcss ./document/main.css --output ./assets/document.css --config ./.config/postcss.config.cjs --verbose",
		"objects": "postcss ./object/main.css --output ./assets/objects.css --config ./.config/postcss.config.cjs --verbose",
		"main": "postcss ./main.css --output ./assets/main.css --config ./.config/postcss.config.cjs --verbose",
		"styles": "postcss ./style/main.css --output ./assets/styles.css --config ./.config/postcss.config.cjs --verbose",
		"utilities": "postcss ./utility/main.css --output ./assets/utilities.css --config ./.config/postcss.config.cjs --verbose",
		"theme-document": "postcss ./theme/document.css --output ./assets/theme-document.css --config ./.config/postcss.config.cjs --verbose"
	},
	"analyze": true,
	"bugs": {
		"url": "https://github.com/BPanchenko/protosite-uikit/issues"
	},
	"preferGlobal": true,
	"private": false,
	"repository": {
		"type": "git",
		"url": "https://github.com/BPanchenko/protosite-uikit.git"
	},
	"devDependencies": {
		"@csstools/custom-units": "0.1.1",
		"cssnano": "5.1.14",
		"cssnano-preset-default": "5.2.13",
		"npm-run-all": "4.1.5",
		"postcss": "8.4.19",
		"postcss-cli": "10.1.0",
		"postcss-custom-media": "9.0.1",
		"postcss-custom-selectors": "7.0.0",
		"postcss-extend-rule": "4.0.0",
		"postcss-import": "15.1.0",
		"postcss-nested": "6.0.0",
		"protosite-palette": "2.13.19"
	},
	"license": "ISC",
	"author": {
		"name": "Boris Panchenko",
		"email": "boris.v.panchenko@mail.ru",
		"url": "https://bpanchenko.ru"
	}
}
