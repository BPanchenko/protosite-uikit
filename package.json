{
  "name": "protosite.uikit",
  "version": "2.4.7-assets",
  "description": "Front-end framework for developing web interfaces using BEMIT",
  "author": "Boris Panchenko <hello@bpanchenko.ru>",
  "main": "./assets/uikit.js",
  "style": "./assets/uikit.css",
  "scripts": {
    "build": "gulp --gulpfile .config/gulpfile.js",
    "start": "npm-run-all --parallel watch:**",
    "watch:js": "gulp watch-js --gulpfile .config/gulpfile.js",
    "watch:css": "postcss ./src/css/main.css --config ./.config/postcss.config.js --output ./assets/uikit.css --watch"
  },
  "analyze": true,
  "browserslist": [
    ">1%",
    "last 4 versions",
    "not ie < 9"
  ],
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite.uikit/issues"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.8",
    "eslint": "^5.16.0",
    "fancy-log": "^1.3.3",
    "gulp": "^4.0.2",
    "gulp-clean": "^0.4.0",
    "gulp-postcss": "^8.0.0",
    "gulp-rename": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-properties": "^8.0.11",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-nested": "^4.2.1",
    "postcss-preset-env": "^6.7.0",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-stream": "^5.2.1"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "esprima",
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [
        2,
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "homepage": "https://github.com/BPanchenko/protosite.uikit#readme",
  "license": "ISC",
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite.uikit.git"
  }
}
