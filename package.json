{
  "name": "protosite.uikit",
  "version": "1.7.41",
  "description": "Front-end framework for developing web interfaces using BEMIT",
  "author": "Boris Panchenko <hello@bpanchenko.ru>",
  "main": "./index.html",
  "scripts": {},
  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite.uikit/issues"
  },
  "devDependencies": {
    "autoprefixer": "^9.4.5",
    "cssnano": "^4.1.8",
    "eslint": "^5.12.1",
    "gulp": "^4.0.0",
    "gulp-postcss": "^8.0.0",
    "postcss": "^7.0.13",
    "postcss-preset-env": "^6.5.0",
    "sugarss": "^2.0.0",
    "webpack": "^4.28.4"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "esprima",
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [
        2,
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "homepage": "https://github.com/BPanchenko/protosite.uikit#readme",
  "license": "ISC",
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite.uikit.git"
  },
  "dependencies": {
    "npm": "^6.6.0"
  }
}
