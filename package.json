{
  "name": "@bpanchenko/uikit",
  "version": "3.4.10",
  "description": "CSS framework for developing web interfaces",
  "homepage": "https://github.com/BPanchenko/protosite-uikit#readme",
  "type": "module",
  "main": "main.css",
  "files": [
    "components/*",
    "document/*",
    "objects/*",
    "settings/*",
    "styles/*",
    "themes/*",
    "utilities/*"
  ],
  "scripts": {
    "build": "postcss ./main.css > ./assets/main.css --config .config/postcss.config.cjs",
    "start": "npm run watch",
    "watch": "postcss ./main.css > ./assets/main.css --config .config/postcss.config.cjs --watch --verbose"
  },
  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-uikit/issues"
  },
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite-uikit.git"
  },
  "devDependencies": {
    "cssnano": "5.1.13",
    "cssnano-preset-default": "5.2.12",
    "postcss": "8.4.18",
    "postcss-cli": "10.0.0",
    "postcss-color-function": "4.1.0",
    "postcss-custom-media": "8.0.2",
    "postcss-custom-properties": "12.1.9",
    "postcss-import": "15.0.0",
    "postcss-nested": "6.0.0",
    "postcss-preset-env": "7.8.2",
    "protosite-palette": "2.9.14"
  },
	"license": "ISC",
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  }
}
