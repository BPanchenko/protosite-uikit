{
  "name": "protosite.uikit",
  "version": "2.4",
  "description": "Front-end framework for developing web interfaces using BEMIT",
  "author": "Boris Panchenko <hello@bpanchenko.ru>",
  "main": "./dist/protosite-uikit.js",
  "style": "./dist/protosite-uikit.css",
  "scripts": {
    "dev": "webpack-dev-server --mode development --config .config/webpack.config.js",
    "dev-postcss": "postcss ./src/css/protosite-uikit.css --config ./.config/postcss.config.js --output ./dist/protosite-uikit.css --watch",
    "build": "webpack --mode production --config .config/webpack.config.js",
    "build-gulp": "gulp build-css --gulpfile .config/gulpfile.js"
  },
  "analyze": true,
  "browserslist": [
    ">1%",
    "last 4 versions",
    "not ie < 9"
  ],
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite.uikit/issues"
  },
  "devDependencies": {
    "autoprefixer": "^9.7.1",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.8",
    "eslint": "^5.16.0",
    "gulp": "^4.0.2",
    "gulp-postcss": "^8.0.0",
    "postcss": "^7.0.21",
    "postcss-cli": "^6.1.3",
    "postcss-color-function": "^4.1.0",
    "postcss-custom-properties": "^8.0.11",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.7.0",
    "style-loader": "^0.23.1",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10",
    "webpack-dev-server": "^3.9.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "esprima",
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [
        2,
        "single",
        {
          "allowTemplateLiterals": true
        }
      ]
    }
  },
  "homepage": "https://github.com/BPanchenko/protosite.uikit#readme",
  "license": "ISC",
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite.uikit.git"
  }
}
