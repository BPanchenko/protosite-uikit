{
  "name": "@bpanchenko/uikit",
  "version": "4.25.68",
  "description": "CSS framework for developing web interfaces",
  "homepage": "https://github.com/BPanchenko/protosite-uikit#readme",
  "type": "module",
  "main": "assets/main.css",
  "types": "assets/main.d.ts",
  "files": [
    ".config/cssnano.config.cjs",
    ".config/postcss.config.cjs",
    ".snippet/*",
    "assets/*",
    "component/*",
    "document/*",
    "object/*",
    "settings/*",
    "style/*",
    "theme/*",
    "utility/*",
    "fonts.css",
    "main.css"
  ],
  "scripts": {
    "build": "run-s prettier clean build:css build:esm prettier",
    "build:css": "node .builder/css.cjs",
    "build:esm": "node .builder/esm.cjs",
    "clean": "rimraf assets",
    "prettier": "prettier --write ."
  },
  "dependencies": {
    "@csstools/custom-units": "0.1.1",
    "cssnano": "6.0.1",
    "cssnano-preset-default": "6.0.1",
    "postcss": "8.4.31",
    "postcss-custom-media": "10.0.2",
    "postcss-custom-selectors": "7.1.6",
    "postcss-extend-rule": "4.0.0",
    "postcss-import": "15.1.0",
    "postcss-nested": "6.0.1",
    "postcss-reporter": "7.0.5",
    "protosite-palette": "2.13.20"
  },
  "devDependencies": {
    "css": "3.0.0",
    "glob": "10.3.10",
    "lodash": "4.17.21",
    "node-color-log": "10.0.2",
    "npm-run-all": "4.1.5",
    "pluralize": "8.0.0",
    "prettier": "3.1.0",
    "rimraf": "5.0.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite-uikit.git"
  },
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-uikit/issues"
  },
  "keywords": [
    "bemit",
    "css",
    "oocss",
    "postcss",
    "uikit"
  ],
