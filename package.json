{
  "name": "protosite.uikit",
  "version": "1.7.41",
  "description": "Front-end framework for developing web interfaces using BEMIT",
  "author": "Boris Panchenko <hello@bpanchenko.ru>",
  "main": "./index.html",

  "scripts": {
    "dev": "rollup -c -w --environment NODE_ENV:dev,PORT:8080",
    "build:dev": "rollup -c -w --environment NODE_ENV:dev",
    "build:prod": "rollup -c --environment NODE_ENV:prod"
  },

  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite.uikit/issues"
  },
  "devDependencies": {
    "autoprefixer": "^8.1.0",
    "cssnano": "^3.10.0",
    "eslint": "^5.2.0",
    "less": "^3.8.0",
    "postcss": "^6.0.20",
    "rollup": "^0.57.1",
    "rollup-plugin-alias": "^1.4.0",
    "rollup-plugin-commonjs": "^9.1.0",
    "rollup-plugin-less": "^0.1.4",
    "rollup-plugin-livereload": "^0.6.0",
    "rollup-plugin-node-globals": "^1.2.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-replace": "^2.0.0",
    "rollup-plugin-serve": "^0.4.2",
    "rollup-plugin-uglify": "^3.0.0"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "esprima",
    "rules": {
      "no-const-assign": "warn",
      "no-this-before-super": "warn",
      "no-unreachable": "warn",
      "no-unused-vars": "warn",
      "constructor-super": "warn",
      "valid-typeof": "warn",
      "quotes": [2, "single", { "allowTemplateLiterals": true }]
    }
  },
  "homepage": "https://github.com/BPanchenko/protosite.uikit#readme",
  "license": "ISC",
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite.uikit.git"
  }
}
