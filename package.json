{
  "name": "@bpanchenko/uikit",
  "version": "3.5.13",
  "description": "CSS framework for developing web interfaces",
  "homepage": "https://github.com/BPanchenko/protosite-uikit#readme",
  "type": "module",
  "main": "main.css",
  "files": [
    "components/*",
    "document/*",
    "objects/*",
    "settings/*",
    "styles/*",
    "themes/*",
    "utilities/*"
  ],
  "scripts": {
    "build": "run-s main document components styles utilities objects theme-article theme-errordoc",
    "components": "postcss ./components/main.css > ./assets/components.css --config ./.config/postcss.config.cjs --verbose",
    "document": "postcss ./document/main.css > ./assets/document.css --config ./.config/postcss.config.cjs --verbose",
    "objects": "postcss ./objects/main.css > ./assets/objects.css --config ./.config/postcss.config.cjs --verbose",
    "main": "postcss ./main.css > ./assets/main.css --config ./.config/postcss.config.cjs --verbose",
    "styles": "postcss ./styles/main.css > ./assets/styles.css --config ./.config/postcss.config.cjs --verbose",
    "utilities": "postcss ./utilities/main.css > ./assets/utilities.css --config ./.config/postcss.config.cjs --verbose",
    "theme-article": "postcss ./themes/article.css > ./assets/theme-article.css --config ./.config/postcss.config.cjs --verbose",
    "theme-errordoc": "postcss ./themes/errordoc.css > ./assets/theme-errordoc.css --config ./.config/postcss.config.cjs --verbose"
  },
  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-uikit/issues"
  },
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite-uikit.git"
  },
  "devDependencies": {
    "cssnano": "5.1.13",
    "cssnano-preset-default": "5.2.12",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.18",
    "postcss-cli": "10.0.0",
    "postcss-custom-media": "8.0.2",
    "postcss-import": "15.0.0",
    "postcss-nested": "6.0.0",
    "postcss-preset-env": "7.8.2",
    "protosite-palette": "2.9.14"
  },
  "license": "ISC",
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  }
}
