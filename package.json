{
  "name": "@bpanchenko/uikit",
  "version": "3.4.7",
  "description": "CSS framework for developing web interfaces",
  "homepage": "https://github.com/BPanchenko/protosite-uikit#readme",
	"type": "module",
  "main": "main.css",
  "scripts": {
    "build": "postcss ./main.css > ./assets/main.css --config .config/postcss.config.js",
    "start": "npm run watch",
    "watch": "postcss ./main.css > ./assets/main.css --config .config/postcss.config.js --watch --verbose"
  },
  "analyze": true,
  "bugs": {
    "url": "https://github.com/BPanchenko/protosite-uikit/issues"
  },
  "preferGlobal": true,
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/BPanchenko/protosite-uikit.git"
  },
  "devDependencies": {
    "cssnano": "5.0.14",
    "cssnano-preset-default": "5.1.11",
    "npm-run-all": "4.1.5",
    "postcss": "8.4.5",
    "postcss-cli": "9.1.0",
    "postcss-color-function": "4.1.0",
    "postcss-custom-media": "8.0.0",
    "postcss-custom-properties": "12.0.2",
    "postcss-import": "14.0.2",
    "postcss-nested": "5.0.6",
    "postcss-preset-env": "7.2.0",
    "protosite-palette": "2.9.14"
  },
  "license": "ISC",
  "author": {
    "name": "Boris Panchenko",
    "email": "boris.v.panchenko@mail.ru",
    "url": "https://bpanchenko.ru"
  }
}
