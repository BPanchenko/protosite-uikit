AddDefaultCharset UTF-8
ServerSignature Off
Options -Indexes

<IfModule mod_gzip.c>
    mod_gzip_on         Yes
    mod_gzip_dechunk    Yes
    mod_gzip_item_include file		\.(html?|json|css|js|php)$
    mod_gzip_item_include mime		^text\.*
    mod_gzip_item_include mime		^application/javascript.*
    mod_gzip_item_include mime		^application/json.*
    mod_gzip_item_exclude mime		^image\.*
    mod_gzip_item_exclude rspheader	^Content-Encoding:.*gzip.*
</IfModule>

<IfModule mod_headers.c>
    Header always set Access-Control-Allow-Origin "*"
    Header always set Access-Control-Allow-Methods "GET"
    Header set Link "<https://fonts.googleapis.com/css?family=Marck+Script|PT+Mono|PT+Serif+Caption|Open+Sans:400,400italic,600,600italic,300,700,800,300italic,800italic,700italic&subset=latin,cyrillic>; rel=preload"
</IfModule>